{
  "name": "buzz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build && start dist/stats.html",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prune-branches": "git fetch --prune && powershell -Command \"$goneBranches = git branch -vv | Select-String ': gone]' | ForEach-Object { if ($_.Line -match '^\\\\s*(\\\\S+)') { $matches[1] } }; foreach ($branch in $goneBranches) { git branch -D $branch }\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@studio-freight/lenis": "^1.0.42",
    "emoji-picker-react": "^4.12.0",
    "firebase": "^10.14.0",
    "framer-motion": "^11.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.0",
    "react-toastify": "^10.0.5",
    "timeago.js": "^4.0.2",
    "yet-another-react-lightbox": "^3.28.0",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@tanstack/react-virtual": "^3.13.18",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
